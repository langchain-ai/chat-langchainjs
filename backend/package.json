{
  "name": "backend",
  "version": "0.0.0",
  "description": "Backend for Chat LangChainJS",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:langchain-ai/chat-langchainjs.git"
  },
  "homepage": "https://github.com/langchain-ai/chat-langchainjs/tree/main/",
  "scripts": {
    "build": "yarn clean && tsup",
    "ingest:dev": "tsx --experimental-wasm-modules -r dotenv/config ./src/ingest.ts dotenv_config_path=.env.local",
    "ingest": "node ./dist/ingest.js",
    "lint:eslint": "NODE_OPTIONS=--max-old-space-size=4096 eslint --cache --ext .ts,.js src/",
    "lint:dpdm": "dpdm --exit-code circular:1 --no-warning --no-tree src/*.ts src/**/*.ts",
    "lint": "yarn lint:eslint && yarn lint:dpdm",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:dpdm",
    "clean": "rimraf dist/",
    "format": "prettier --config .prettierrc --write \"src\"",
    "format:check": "prettier --config .prettierrc --check \"src\""
  },
  "author": "LangChain",
  "license": "MIT",
  "dependencies": {
    "@langchain/community": "^0.0.27",
    "@langchain/core": "^0.1.27",
    "@langchain/openai": "^0.0.14",
    "@langchain/weaviate": "^0.0.1",
    "cheerio": "^1.0.0-rc.12",
    "jsdom": "^24.0.0",
    "langchain": "^0.1.17",
    "pg": "^8.11.3",
    "weaviate-ts-client": "^2.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@langchain/scripts": "~0.0",
    "@swc/core": "^1.3.90",
    "@swc/jest": "^0.2.29",
    "@types/pg": "^8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "dotenv": "^16.4.1",
    "dpdm": "^3.12.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.6.4",
    "prettier": "^2.8.3",
    "release-it": "^15.10.1",
    "rimraf": "^5.0.1",
    "tsup": "^8.0.1",
    "tsx": "^3.12.3",
    "typescript": "~5.1.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "llm",
    "ai",
    "gpt3",
    "chain",
    "prompt",
    "prompt engineering",
    "chatgpt",
    "machine learning",
    "ml",
    "openai",
    "embeddings",
    "vectorstores"
  ],
  "files": [
    "dist/",
    "index.cjs",
    "index.js",
    "index.d.ts",
    "index.d.cts"
  ]
}
